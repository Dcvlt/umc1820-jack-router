{
  "name": "umc1820-jack-router",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server.js",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose --profile dev up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "test": "echo \"Tests not implemented\" && exit 0",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/register": "^7.27.1",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.525.0",
    "mqtt": "^5.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "vite": "^4.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {}
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
